<script type="text/javascript">
    function showWarningIfBrowserUnsupported() {
        try{
            var userAgentMatchesSupportedBrowsers = new RegExp(supportedBrowsersRegExp).test(navigator.userAgent);

            if(!userAgentMatchesSupportedBrowsers) {
                document.getElementById("browser-unsupported-warning").style.display = "block"
            }

            if("undefined"!==typeof(LUX)){
                LUX.addData("browserSupported", userAgentMatchesSupportedBrowsers.toString());
            }

        } catch( error) {
            console.error(error);
        }
    }
</script>
