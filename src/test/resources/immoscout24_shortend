        </div>
        <script> (function () {
          const dropdown = document.querySelector('#is24-dropdown');
          const USER_TYPE_LOCAL_STORAGE_KEY = 'userType';
          const DEFAULT_USER_TYPE = 'consumer';
          const getItem = (key) => {
            try {
              return localStorage.getItem(key);
            } catch (e) {
              return null;
            }
          };
          const setItem = (key, value) => {
            try {
              localStorage.setItem(key, value);
            } catch (e) {
            }
          };
          const getStoredUserType = () => getItem(USER_TYPE_LOCAL_STORAGE_KEY);
          const getComponentsConfig = () => {
            return window?.IS24?.navigation?.config;
          };
          const getUserType = () => {
            const forced = getComponentsConfig()?.userType;
            if (forced) {
              return forced;
            }
            const storedUserType = getStoredUserType();
            if (storedUserType) {
              return storedUserType;
            }
            return DEFAULT_USER_TYPE;
          };
          const setUserType = (userType) => {
            setItem(USER_TYPE_LOCAL_STORAGE_KEY, userType);
          };
          const SANDBOX_DOMAIN = 'sandbox-immobilienscout24.de';
          const PRODUCTION_DOMAIN = 'immobilienscout24.de';
          const isSandbox = () => window.location.host.includes(SANDBOX_DOMAIN);
          const isProd = () => !isSandbox() && window.location.host.includes(PRODUCTION_DOMAIN);
          const getDomain = () => {
            if (isSandbox()) {
              return SANDBOX_DOMAIN;
            }
            if (isProd()) {
              return PRODUCTION_DOMAIN
            }
            return window.location.host
          };
          const DOMAIN = getDomain();
          const links = dropdown.querySelectorAll('a[href*="domain_placeholder"]');
          links.forEach(link => {
            link.href = link.href.replace('domain_placeholder', DOMAIN)
          })
          const tabs = dropdown.querySelectorAll('.Tabs_Tab__3Iqu-');
          const tabPanels = dropdown.querySelectorAll('.Tabs_PanelsContainer__3rK3O > [role="tabpanel"]');
          const showTab = (userType) => {
            tabs.forEach((tab) => {
              if (tab.dataset.userType !== userType) {
                tab.setAttribute('aria-selected', 'false')
              } else {
                tab.setAttribute('aria-selected', 'true')
              }
            })
            tabPanels.forEach((panel, j) => {
              if (panel.dataset.userType !== userType) {
                panel.setAttribute('hidden', true);
              } else {
                panel.removeAttribute('hidden');
              }
            })
          }
          tabs.forEach((tab, i) => {
            tab.addEventListener('click', (e) => {
              showTab(e.currentTarget.dataset.userType)
            })
          })
          const initUserType = () => {
            const user = getUserType();
            setUserType(user);
            showTab(user);
          };
          initUserType();
          const getLogOutLink = (url) => {
            const parsedUrl = new URL(url);
            const ssoReturnUrl = window?.IS24?.ssoReturnUrl ? window.IS24.ssoReturnUrl : window.location.href;
            const ssoAppName = window?.IS24?.ssoAppName ? window.IS24.ssoAppName : 'is24main'
            parsedUrl.searchParams.set('sso_return', ssoReturnUrl)
            parsedUrl.searchParams.set('appName', ssoAppName)
            return parsedUrl.href;
          }
          const trackingLinks = {
            anmelden: {trackingParams: {evt_ga_category: 'header', evt_ga_action: 'meinkonto', evt_ga_label: 'anmelden'}},
            consumer_Ubersicht: {trackingParams: {evt_ga_action: 'meinkonto', evt_ga_label: 'mein-bereich'}},
            consumer_Nachrichten: {trackingParams: {evt_ga_action: 'meinkonto', evt_ga_label: 'nachrichten'}},
            consumer_Gespeichert: {trackingParams: {evt_ga_category: 'header', evt_ga_action: 'meinkonto', evt_ga_label: 'gespeichert'}},
            consumer_Profil: {trackingParams: {evt_ga_action: 'meinkonto', evt_ga_label: 'persoenliches-profil'}},
            provider_Ubersicht: {
              trackingParams: {evt_ga_action: 'meinkonto', evt_ga_label: 'mein-bereich'}, onClick: (e) => {
                setUserType('provider')
              }
            },
            provider_Nachrichten: {trackingParams: {evt_ga_action: 'meinkonto', evt_ga_label: 'nachrichten'}},
            provider_Immobilienanzeigen: {trackingParams: {evt_ga_action: 'meinkonto', evt_ga_label: 'immobilienanzeigenn'},},
            konto_verwalten: {trackingParams: {evt_ga_action: 'meinkonto', evt_ga_label: 'setting'},},
            abmelden: {trackingParams: {evt_ga_action: 'meinkonto', evt_ga_label: 'abmelden'},},
            kunde_werden: {trackingParams: {evt_ga_action: 'meinkonto', evt_ga_label: 'anbieter'},}
          }
          const logOutLinks = dropdown.querySelectorAll('[data-log-out]');
          logOutLinks.forEach((link) => {
            link.href = getLogOutLink(link.href);
            link.addEventListener('click', async (e) => {
              const element = e.currentTarget;
              element.href = getLogOutLink(element.href);
            })
          })
          const trackedLinks = dropdown.querySelectorAll('a[data-tracked-link]');
          trackedLinks.forEach(link => {
            link.addEventListener('click', async (e) => {
              e.preventDefault();
              const element = e.currentTarget;
              try {
                const {trackingParams, onClick} = trackingLinks[element.dataset.trackedLink];
                if (onClick) {
                  onClick();
                }
                if (trackingParams) {
                  await Promise.race([new Promise(resolve => setTimeout(resolve, 300)), new Promise(resolve => {
                    window.IS24.TEALIUM.tracking.report(trackingParams, {callback: resolve});
                  })]);
                }
              } catch (e) {
              }
              const target = element.getAttribute('target') ?? '_self';
              const href = element.href;
              window.open(href, target);
            })
          })
          fetch('https://www.' + DOMAIN + '/nachrichten/conversations?clientVersion=3018&maxCount=1').then(response => response.json()).then(data => {
            if (!data.numberOfUnreadConversations) {
              return;
            }
            dropdown.querySelector('.NavigationMenuV2_LinkRight__20i5u').removeAttribute('hidden');
            dropdown.querySelector('.MessengerInfo_Number__1jNPf').innerHTML = data.numberOfUnreadConversations;
          }).catch((e) => {
          })
